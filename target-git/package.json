{
  "name": "@feedlify/target-git",
  "version": "0.0.2",
  "description": "Keeps up-to-date a @feedlify repository from a git compatible location",
  "main": "lib/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel src --out-dir lib",
    "start": "./node_modules/.bin/babel --watch src --out-dir lib",
    "test": "./node_modules/.bin/jest -i",
    "release": "npm run build && npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcopeg/feedlify.git"
  },
  "keywords": [
    "feedlify",
    "feed",
    "aggregator",
    "git",
    "github"
  ],
  "author": "Marco Pegoraro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcopeg/feedlify/issues"
  },
  "homepage": "https://github.com/marcopeg/feedlify/target-git/README.md",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "jest": "^24.5.0",
    "jest-fetch-mock": "^2.1.1"
  },
  "dependencies": {
    "fs-extra": "^7.0.1",
    "@feedlify/git": "^0.0.1",
    "@feedlify/target-fs": "^0.0.2"
  }
}
