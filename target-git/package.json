{
  "name": "@feedlify/target-git",
  "version": "0.0.0",
  "description": "Keeps up-to-date a git based feed definition",
  "main": "lib/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel src --out-dir lib",
    "start": "./node_modules/.bin/babel --watch src --out-dir lib",
    "test": "./node_modules/.bin/jest",
    "release": "npm run build && npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/feedlify/target-git.git"
  },
  "keywords": [
    "feedlify",
    "feed",
    "aggregator",
    "git",
    "github"
  ],
  "author": "Marco Pegoraro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/feedlify/target-git/issues"
  },
  "homepage": "https://github.com/feedlify/target-git#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "fs-extra": "^7.0.1",
    "jest": "^24.5.0",
    "jest-fetch-mock": "^2.1.1"
  },
  "dependencies": {
    "@feedlify/feed-aggregator": "^0.0.0",
    "@feedlify/git": "^0.0.0"
  }
}
