{
  "name": "@feedlify/feed-aggregator",
  "version": "0.0.0",
  "description": "Builds a feed file with fresh data",
  "main": "lib/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel src --out-dir lib",
    "start": "./node_modules/.bin/babel --watch src --out-dir lib",
